\NH\\Desktop\\repo\\learn-react\\songs\\src\\components\\SongList.js",[],"C:\\Users\\NH\\Desktop\\repo\\learn-react\\songs\\src\\components\\SongDetail.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\index.js",[],["92","93"],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\App.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\reducers\\index.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\actions\\index.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\Doll\\DollList.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\Doll\\DollCard.js",["94"],"import React from 'react';\r\nimport { Card, Icon, Image } from 'semantic-ui-react';\r\nimport DollDetailModal from './DollDetailModal';\r\n\r\nconst DollCard = prop => {\r\n\r\n    const { doll } = prop;\r\n    const { specialYn, img, name, type, series, date } = doll;\r\n\r\n    const showSpecialTag = (specialYn) => {\r\n        const specialCorner = { as: 'a', corner: 'left', icon: 'star', color: 'yellow' };\r\n        return (specialYn === 'Y') ? specialCorner : false;\r\n    };\r\n\r\n    return (\r\n        <Card>\r\n            <DollDetailModal\r\n                trigger={<Image src={img} wrapped ui={false} fluid label={showSpecialTag(specialYn)} />}\r\n                doll={doll} />\r\n            <Card.Content>\r\n                <Card.Header>{name} {type}</Card.Header>\r\n                <Card.Meta>{series}\r\n                </Card.Meta>\r\n                <Card.Description>\r\n                    <span className='date'>{date}</span>\r\n                </Card.Description>\r\n            </Card.Content>\r\n        </Card>\r\n    );\r\n};\r\n\r\nexport default DollCard;","C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\actions\\types.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\reducers\\filtersReducer.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\reducers\\dollsReducer.js",["95"],"import {\r\n    FETCH_DOLLS,\r\n    SEARCH_DOLLS,\r\n    FETCH_DOLL,\r\n    FILTER_DOLLS\r\n} from \"../actions/types\";\r\nimport data from \"../data/kukuclara.json\";\r\nimport Pluralize from 'pluralize';\r\n\r\nconst loadedData = JSON.parse(JSON.stringify(data));\r\n\r\n// Function to set primary key(id) to OBJ key\r\nconst parseObjWithKeys = (dolls) => {\r\n    const dollObj = {};\r\n    dolls.forEach(doll => dollObj[doll.id] = doll);\r\n    return dollObj;\r\n}\r\n\r\nexport default (state = [], action) => {\r\n    switch (action.type) {\r\n        case FETCH_DOLLS:\r\n            const fetchedDolls = parseObjWithKeys(loadedData);\r\n            return { ...fetchedDolls };\r\n        case SEARCH_DOLLS:\r\n            const { term } = action.payload;\r\n            if (typeof term === \"undefined\") return parseObjWithKeys(loadedData);\r\n            const foundDolls = loadedData.filter(doll => {\r\n                const { type, series, name } = doll;\r\n                const fullname = `${series} ${name} ${type}`;\r\n                return fullname.toLowerCase().indexOf(term.toLowerCase()) > -1;\r\n            });\r\n            return { ...parseObjWithKeys(foundDolls) };\r\n        case FILTER_DOLLS:\r\n            const submitData = action.payload;\r\n            const selectedFilterKeys = Object.keys(submitData);\r\n            // TODO: Make it less complecated(if possibleðŸ˜­)\r\n            // Retrieve the loaded doll data to filter it\r\n            const filteredDolls = loadedData.filter(doll => {\r\n\r\n                const matchedFilterKeys = selectedFilterKeys.filter(keyName => {\r\n                    // Change/Match to the loaded object's(dolls) property name(e.g. types => typeCode)\r\n                    const parsedKeyName = `${(Pluralize.isSingular(keyName)) ? keyName: Pluralize.singular(keyName)}Code`;\r\n                    const codeFromLoadedData = doll[parsedKeyName];\r\n                    const codesFromSubmitData = submitData[keyName];\r\n                    // Check if one is matching at least\r\n                    // In other words, within one category(e.g. skin, hair color) it is OR(union)\r\n                    return codesFromSubmitData.indexOf(codeFromLoadedData) > -1;\r\n                });\r\n\r\n                // It is AND(intersection) between different categories\r\n                return matchedFilterKeys.sort().join(\",\") === selectedFilterKeys.sort().join(\",\");\r\n            });\r\n\r\n            return { ...parseObjWithKeys(filteredDolls) };\r\n\r\n        case FETCH_DOLL:\r\n            return { ...state, [action.payload.id]: action.payload };\r\n        default:\r\n            return state;\r\n    }\r\n};","C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\UI\\MenuBar.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\UI\\FilterDropdown.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\UI\\FilterModal.js",[],"C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\components\\Doll\\DollDetailModal.js",["96","97"],"import React from 'react'\r\nimport { Button, Header, Image, Modal, Icon } from 'semantic-ui-react'\r\n\r\nconst DollDetailModal = props => {\r\n  const [open, setOpen] = React.useState(false);\r\n  const { trigger, doll } = props;\r\n  const { specialYn, img, name, type, series, date } = doll;\r\n\r\n  return (\r\n    <Modal\r\n      onClose={() => setOpen(false)}\r\n      onOpen={() => setOpen(true)}\r\n      open={open}\r\n      trigger={trigger}\r\n      size='small'\r\n    >\r\n      <Modal.Header>Select a Photo</Modal.Header>\r\n      <Modal.Content image>\r\n        <Image size='medium' src={img} wrapped />\r\n        <Modal.Description>\r\n          <Header>{name} {type}</Header>\r\n          <p>\r\n            {series}\r\n          </p>\r\n          <p>Is it okay to use this photo?</p>\r\n        </Modal.Description>\r\n      </Modal.Content>\r\n      <Modal.Actions>\r\n        <Button color='black' onClick={() => setOpen(false)}>\r\n          <Icon name='remove' /> Close\r\n        </Button>\r\n      </Modal.Actions>\r\n    </Modal>\r\n  )\r\n}\r\n\r\nexport default DollDetailModal","C:\\Users\\NH\\Desktop\\repo\\kukuclara\\src\\common\\util.js",[],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"message":"103","line":2,"column":8,"nodeType":"104","messageId":"105","endLine":2,"endColumn":15},{"ruleId":"102","severity":1,"message":"106","line":3,"column":10,"nodeType":"104","messageId":"105","endLine":3,"endColumn":20},{"ruleId":"98","replacedBy":"107"},{"ruleId":"100","replacedBy":"108"},{"ruleId":"102","severity":1,"message":"109","line":2,"column":16,"nodeType":"104","messageId":"105","endLine":2,"endColumn":20},{"ruleId":"110","severity":1,"message":"111","line":19,"column":1,"nodeType":"112","endLine":61,"endColumn":3},{"ruleId":"102","severity":1,"message":"113","line":7,"column":11,"nodeType":"104","messageId":"105","endLine":7,"endColumn":20},{"ruleId":"102","severity":1,"message":"114","line":7,"column":47,"nodeType":"104","messageId":"105","endLine":7,"endColumn":51},"no-native-reassign",["115"],"no-negated-in-lhs",["116"],"no-unused-vars","'actions' is defined but never used.","Identifier","unusedVar","'selectSong' is defined but never used.",["115"],["116"],"'Icon' is defined but never used.","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","'specialYn' is assigned a value but never used.","'date' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]